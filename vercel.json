{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "functions": {
    "api/main.go": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/main.go"
    }
  ],
  "regions": ["sin1"]
}